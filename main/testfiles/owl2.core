		 _checkUnionOf(?c ?r)
		 ?r[rdf:rest->?l]
		 Not(?l = rdf:nil) ))

 Forall ?c ?ci ?l (
   ?ci[rdfs:subClassOf->?c] :- And (
		 _checkUnionOf(?c ?l)
		 ?l[rdf:first->?ci] ))
 
