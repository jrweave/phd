(* <#scm-int> *)
 Forall ?c ?l (
	 _markAllTypes(?c ?l) :- ?c[owl:intersectionOf->?l] )

 Forall ?c ?l ?r (
	 _markAllTypes(?c ?l) :- And (
		 _markAllTypes(?c ?r)
		 ?r[rdf:rest->?l]
		 Not(?l = rdf:nil) ))

 Forall ?c ?ci ?l (
   ?c[rdfs:subClassOf->?ci] :- And (
		 _markAllTypes(?c ?l)
		 ?l[rdf:first->?ci] ))

(* <#scm-uni> *)
